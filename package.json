{
	"name": "fullcourtreport",
	"version": "2.0.0",
	"scripts": {
		"js": "webpack",
		"scss": "node-sass src/scss -o public/assets/css",
		"watch": "npm-watch"
	},
	"watch": {
		"js": "src/js/**/*.js",
		"scss": {
			"patterns": [
				"src/scss"
			],
			"extensions": "scss"
		}
	},
	"dependencies": {
		"express": "^4.16.1",
		"memcached": "^2.2.2",
		"moment": "^2.18.1",
		"moment-timezone": "^0.5.13",
		"react": "^16.0.0",
		"react-dom": "^16.0.0",
		"react-router-dom": "^4.2.2",
		"string": "^3.3.3",
		"zlib": "^1.0.5"
	},
	"devDependencies": {
		"babel-core": "^6.26.0",
		"babel-loader": "^7.1.2",
		"babel-preset-es2015": "^6.24.1",
		"babel-preset-react": "^6.24.1",
		"node-sass": "^4.5.3",
		"npm-watch": "^0.3.0",
		"webpack": "^3.7.1"
	}
}
